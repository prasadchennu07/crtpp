def minion_game(string):
    k=0
    s=0
    v="AEIOU"
    for i in range(len(string)):
        if string[i] in v:
            k+=len(string)-i
        else:
            s+=len(string)-i
    if k>s:
        print("kevin is the winner",k)
    elif s>k:
        print("stuart is the winner",s)
    else:
        print("It is a draw")
string = input()
minion_game(string)
